{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfncplsandip"
		},
		"LinkDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkDataLake'"
		},
		"linkAzureSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkAzureSql'"
		},
		"LinkDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagencplaccount.dfs.core.windows.net/"
		},
		"linkAzureSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sandipserverncpl.database.windows.net"
		},
		"linkAzureSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "databrickprojectdb"
		},
		"linkAzureSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sandip"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfncplsandip_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkDataLake_accountKey": {
						"value": "[parameters('LinkDataLake_accountKey')]"
					},
					"linkAzureSql_password": {
						"value": "[parameters('linkAzureSql_password')]"
					},
					"LinkDataLake_properties_typeProperties_url": {
						"value": "[parameters('LinkDataLake_properties_typeProperties_url')]"
					},
					"linkAzureSql_properties_typeProperties_server": {
						"value": "[parameters('linkAzureSql_properties_typeProperties_server')]"
					},
					"linkAzureSql_properties_typeProperties_database": {
						"value": "[parameters('linkAzureSql_properties_typeProperties_database')]"
					},
					"linkAzureSql_properties_typeProperties_userName": {
						"value": "[parameters('linkAzureSql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}